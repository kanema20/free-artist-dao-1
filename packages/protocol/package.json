{
  "name": "@goldfinch-eng/protocol",
  "version": "0.1.0",
  "description": "A lending protocol built on the blockchain",
  "main": "index.js",
  "scripts": {
    "start": "npx hardhat node --hostname 0.0.0.0 --export-all deployments/all_dev.json",
    "deploy": "BACKER_MERKLE_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/backerMerkleDistributor/merkleDistributorInfo.dev.json BACKER_MERKLE_DIRECT_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/backerMerkleDirectDistributor/merkleDirectDistributorInfo.dev.json MERKLE_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/merkleDistributor/merkleDistributorInfo.dev.json MERKLE_DIRECT_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/merkleDirectDistributor/merkleDirectDistributorInfo.dev.json npx hardhat deploy --export-all deployments/all_dev.json",
    "start:murmuration": "HARDHAT_FORK=mainnet MURMURATION=yes npm run start",
    "start:murmuration:liquidity-mining": "HARDHAT_FORK= MURMURATION=yes npm run start",
    "test": "NODE_OPTIONS=--unhandled-rejections=strict NODE_ENV=test npx hardhat test $(find test/ -name '*.test.ts' | grep -v mainnet_forking) --show-stack-traces",
    "test:unfiltered": "NODE_OPTIONS=--unhandled-rejections=strict NODE_ENV=test npx hardhat test --show-stack-traces",
    "test:mainnet-forking": "NODE_OPTIONS=--unhandled-rejections=strict HARDHAT_FORK=mainnet NODE_ENV=test npx hardhat test $(find test/mainnet_forking/ -name '*.test.ts') --show-stack-traces",
    "test:tenderly": "NODE_OPTIONS=--unhandled-rejections=strict HARDHAT_FORK=mainnet NODE_ENV=test npx hardhat test $(find test/mainnet_forking/ -name '*.test.ts') --show-stack-traces --network localhost",
    "build": "npx hardhat compile",
    "pre-commit": "npx lint-staged",
    "deploy-for-client-test": "npm run generate-merkle-root:client-test && MERKLE_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/merkleDistributor/merkleDistributorInfo.client_test.json MERKLE_DIRECT_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/merkleDirectDistributor/merkleDirectDistributorInfo.client_test.json BACKER_MERKLE_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/backerMerkleDistributor/merkleDistributorInfo.client_test.json BACKER_MERKLE_DIRECT_DISTRIBUTOR_INFO_PATH=./blockchain_scripts/merkle/backerMerkleDirectDistributor/merkleDirectDistributorInfo.client_test.json npx hardhat deploy --export-all deployments/all_client_test.json",
    "lint": "npm run lint-tsc && eslint --resolve-plugins-relative-to=. .",
    "lint:fix": "eslint --resolve-plugins-relative-to=. . --fix && prettier . --write && solhint 'contracts/**/*.sol' --fix",
    "docs": "npx hardhat docgen",
    "verify": "env-cmd -f ../../.env.local npx hardhat etherscan-verify --api-key $ETHERSCAN_API_KEY --solc-input --network",
    "autotask-update": "node autotasks/deployAutoTasks.js",
    "advance-time": "npx hardhat run ./blockchain_scripts/advanceTime.ts --network localhost",
    "generate-manifest": "npx hardhat run ./blockchain_scripts/deployHelpers/generateHardhatUpgradesManifest.ts --network mainnet",
    "pool-admin": "npx hardhat run ./blockchain_scripts/tranchedPoolAdmin.ts --network",
    "verify-contracts-access-control": "npx hardhat run ./blockchain_scripts/verifyContractsAccessControl.ts --network mainnet",
    "tsc": "tsc",
    "lint-tsc": "echo 'Confirming that grep supports Perl regexes...' | grep -P Perl && if test $(npm run tsc | grep -vcP '^((node_modules\\/.+(\\.d)?\\.ts)|(typechain\\/.+\\.d\\.ts)|(blockchain_scripts\\/.+Cannot find module.+all_dev\\.json)|>|$)') -eq 0; then echo 'No unexpected tsc errors. Success.' && exit 0; else echo 'Unexpected tsc errors. Debug by considering the files in tsc output that are not ignored by our regex:' && npm run tsc | grep -vP '^((node_modules\\/.+(\\.d)?\\.ts)|(typechain\\/.+\\.d\\.ts)|(blockchain_scripts\\/.+Cannot find module.+all_dev\\.json)|>|$)' && exit 1; fi",
    "pre-push": "",
    "_generate-merkle-distributor-grants:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/merkleDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/merkleDistributor/grants.dev.json",
    "_generate-backer-merkle-distributor-grants:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/backerMerkleDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/backerMerkleDistributor/grants.dev.json",
    "_generate-merkle-distributor-grants:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/merkleDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/merkleDistributor/grants.client_test.json",
    "_generate-backer-merkle-distributor-grants:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/backerMerkleDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/backerMerkleDistributor/grants.client_test.json",
    "_generate-merkle-direct-distributor-grants:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/merkleDirectDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/merkleDirectDistributor/grants.dev.json",
    "_generate-backer-merkle-direct-distributor-grants:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.dev.json",
    "_generate-merkle-direct-distributor-grants:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/merkleDirectDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/merkleDirectDistributor/grants.client_test.json",
    "_generate-backer-merkle-direct-distributor-grants:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateGrantsFromTemplate -i ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.dev.template.json -o ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.client_test.json",
    "_generate-merkle-distributor-info:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/merkleDistributor/grants.dev.json -o ./blockchain_scripts/merkle/merkleDistributor/merkleDistributorInfo.dev.json",
    "_generate-backer-merkle-distributor-info:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/backerMerkleDistributor/grants.dev.json -o ./blockchain_scripts/merkle/backerMerkleDistributor/merkleDistributorInfo.dev.json",
    "_generate-merkle-distributor-info:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/merkleDistributor/grants.client_test.json -o ./blockchain_scripts/merkle/merkleDistributor/merkleDistributorInfo.client_test.json",
    "_generate-backer-merkle-distributor-info:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/backerMerkleDistributor/grants.client_test.json -o ./blockchain_scripts/merkle/backerMerkleDistributor/merkleDistributorInfo.client_test.json",
    "_generate-merkle-direct-distributor-info:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/merkleDirectDistributor/grants.dev.json -o ./blockchain_scripts/merkle/merkleDirectDistributor/merkleDirectDistributorInfo.dev.json",
    "_generate-backer-merkle-direct-distributor-info:local": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.dev.json -o ./blockchain_scripts/merkle/backerMerkleDirectDistributor/merkleDirectDistributorInfo.dev.json",
    "_generate-merkle-direct-distributor-info:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/merkleDirectDistributor/grants.client_test.json -o ./blockchain_scripts/merkle/merkleDirectDistributor/merkleDirectDistributorInfo.client_test.json",
    "_generate-backer-merkle-direct-distributor-info:client-test": "node --require hardhat/register ./blockchain_scripts/merkle/merkleDirectDistributor/generateMerkleRoot -i ./blockchain_scripts/merkle/backerMerkleDirectDistributor/grants.client_test.json -o ./blockchain_scripts/merkle/backerMerkleDirectDistributor/merkleDirectDistributorInfo.client_test.json",
    "_generate-merkle-grants:local": "npm run _generate-merkle-distributor-grants:local && npm run _generate-merkle-direct-distributor-grants:local",
    "_generate-backer-merkle-grants:local": "npm run _generate-backer-merkle-distributor-grants:local && npm run _generate-backer-merkle-direct-distributor-grants:local",
    "_generate-merkle-grants:client-test": "npm run _generate-merkle-distributor-grants:client-test && npm run _generate-merkle-direct-distributor-grants:client-test",
    "_generate-backer-merkle-grants:client-test": "npm run _generate-backer-merkle-distributor-grants:client-test && npm run _generate-backer-merkle-direct-distributor-grants:client-test",
    "_generate-merkle-info:local": "npm run _generate-merkle-distributor-info:local && npm run _generate-merkle-direct-distributor-info:local",
    "_generate-backer-merkle-info:local": "npm run _generate-backer-merkle-distributor-info:local && npm run _generate-backer-merkle-direct-distributor-info:local",
    "_generate-merkle-info:client-test": "npm run _generate-merkle-distributor-info:client-test && npm run _generate-merkle-direct-distributor-info:client-test",
    "_generate-backer-merkle-info:client-test": "npm run _generate-backer-merkle-distributor-info:client-test && npm run _generate-backer-merkle-direct-distributor-info:client-test",
    "generate-merkle-root:local": "npm run _generate-merkle-grants:local && npm run _generate-backer-merkle-grants:local && npm run _generate-merkle-info:local && npm run _generate-backer-merkle-info:local",
    "generate-merkle-root:murmuration": "npm run generate-merkle-root:local",
    "generate-merkle-root:client-test": "MERKLE_GRANT_RECIPIENTS=0x0000000000000000000000000000000000000000 npm run _generate-merkle-grants:client-test && MERKLE_GRANT_RECIPIENTS=0x0000000000000000000000000000000000000000 npm run _generate-backer-merkle-grants:client-test && npm run _generate-merkle-info:client-test && npm run _generate-backer-merkle-info:client-test"
  },
  "lint-staged": {
    "*.(js(x)?|ts(x)?)": [
      "prettier --write",
      "eslint --resolve-plugins-relative-to=. --fix"
    ],
    "*.sol": [
      "prettier --write",
      "solhint --fix"
    ]
  },
